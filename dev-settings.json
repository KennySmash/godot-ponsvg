{
  "godot_dev_path": "E:\\Dev\\godot-dev",
  "module_name": "ponsvg",
  "build_target": "editor",
  "platform": "windows",
  "scons_options": {
    "target": "editor",
    "platform": "windows", 
    "module_ponsvg_enabled": "yes",
    "optimize": "speed",
    "warnings": "extra",
    "werror": "no"
  },
  "build_output": {
    "create_timestamped_builds": true,
    "keep_build_history": true,
    "max_old_builds": 5,
    "include_debug_symbols": false,
    "package_with_templates": false
  },
  "test_settings": {
    "enable_performance_tests": true,
    "enable_visual_tests": true,
    "test_assets_path": "tests/assets",
    "output_path": "tests/output"
  },  "development": {
    "auto_copy_module": true,
    "auto_build_after_copy": true,
    "backup_existing_module": true,
    "clean_build": false
  }
}
